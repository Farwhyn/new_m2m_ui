<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="anchorpane" prefHeight="600.0" prefWidth="900.0" styleClass="background-login" stylesheets="@loginTheme.css" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.LoginController">
   <children>
      <HBox layoutX="532.0" layoutY="367.0">
         <children>
            <Label prefHeight="37.0" prefWidth="136.0" stylesheets="@loginTheme.css" text="Username" />
            <TextField fx:id="username" prefHeight="37.0" prefWidth="178.0" />
         </children></HBox>
      <HBox layoutX="532.0" layoutY="414.0">
         <children>
            <Label prefHeight="38.0" prefWidth="138.0" text="Password" />
            <PasswordField fx:id="password" prefHeight="37.0" prefWidth="177.0" />
         </children></HBox>
      <Button fx:id="loginButton" layoutX="762.0" layoutY="485.0" mnemonicParsing="false" onAction="#handleLoginRequest" stylesheets="@loginTheme.css" text="Login" />
   </children>
</AnchorPane>
